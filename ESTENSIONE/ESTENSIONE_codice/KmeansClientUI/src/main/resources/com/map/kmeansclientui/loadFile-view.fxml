<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="600.0" prefWidth="534.0" style="-fx-background-color: #1B262C;" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.map.kmeansclientui.LoadFileViewController">
   <children>
      <VBox prefHeight="600.0" prefWidth="534.0" spacing="50.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <HBox prefHeight="70.0" prefWidth="534.0" spacing="20.0">
               <children>
                  <VBox prefHeight="200.0" prefWidth="100.0" spacing="5.0">
                     <children>
                        <TextField fx:id="tabName" prefHeight="36.0" promptText="Tabella" styleClass="text-field" stylesheets="@style.css" />
                        <TextField fx:id="k" layoutX="10.0" layoutY="10.0" prefHeight="36.0" promptText="Iterate" styleClass="text-field" stylesheets="@style.css" />
                     </children>
                  </VBox>
                  <Button fx:id="loadBtn" mnemonicParsing="false" onAction="#learningFromFile" stylesheets="@style.css" text="Carica">
                     <HBox.margin>
                        <Insets top="20.0" />
                     </HBox.margin>
                  </Button>
               </children>
               <padding>
                  <Insets left="50.0" top="30.0" />
               </padding>
            </HBox>
            <StackPane fx:id="printPane" />
         </children>
      </VBox>
   </children>
</AnchorPane>
